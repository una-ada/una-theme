  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#555f47"
      }
    },
    {
      "name": "Punctuation,",
      "scope": ["punctuation", "meta.brace"],
      "settings": {
        "foreground": "#555f47"
      }
    },
    {
      "name": "Variables, Tags",
      "scope": [
        "variable",
        "support.other.variable",
        "string constant.other.placeholder",
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#d68067"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#bf3041"
      }
    },
    {
      "name": "Keyword, Storage, Operator",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "punctuation.definition.block.tag.jsdoc",
        "keyword.control",
        "meta.tag",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#145c62"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#dccca1"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.escape",
        "constant.other.color",
        "keyword.other.unit",
        "keyword.other",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#703d41"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "support.constant",
        "constant.other.symbol",
        "constant.other.key",
        "markup.inserted.git_gutter"
      ],
      "settings": {
        "foreground": "#729a89"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.other.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types",
        "variable.other.property",
        "variable.other.object.property"
      ],
      "settings": {
        "foreground": "#e0c1bf"
      }
    },
    {
      "name": "Markup - Italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#729a89"
      }
    },
    {
      "name": "Markup - Bold",
      "scope": ["markup.bold", "markup.bold string"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#729a89"
      }
    }
  ]
}
